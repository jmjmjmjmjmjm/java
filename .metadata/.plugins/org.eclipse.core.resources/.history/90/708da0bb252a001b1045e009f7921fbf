package composit;

import lombok.Data;

@Data

public class Coke {
		
	private int price;
	private String desc;
	
	public Coke(int price, String desc) {

		this.price = price;
		this.desc = desc;
	}
	
	public Coke() {
	this(1000,"기본버거");
		
	}
	
	
	
	
	public static void main(String[] args) {

	}





	

}
