package sms;

public class Response {
	
	String group_id;
	String error_list;
	String sucess_count;
	String error_count;
	
	public static void main(String[] args) {
		String data ="{\"group_id\":\"R2Gy0FRd0uPAupcw\",\"error_list\":{\"1\":\"1062\"},\"success_count\":0,\"error_count\":1}";
				
		Response res =new Response();
		
		data.split(",");
		data.split("\"");
		
		System.out.println(data);
	}
}
