package Ch05;

class Animal{
	void sound() {}
}

class Dog extends Animal{
//	오버라이딩 (무효화하다-부모의 함수)
	void sound() {
		System.out.println("멍멍");
	}
}

class Cat extends Animal{
//	오버라이딩 (무효화하다-부모의 함수)
	void sound() {
		System.out.println("냐옹");
	}
}

class Bird extends Animal{
	void sound() {
//		오버라이딩 (무효화하다-부모의 함수) 동적 바인딩
		System.out.println("짹짹");
	}
}


public class Inherit04 {

}
