package conn;

import java.net.MalformedURLException;
import java.net.URL;

import javax.net.ssl.HttpsURLConnection;

public class Conn01 {
	public static void main(String[] args) {
		try {
			URL url = new URL("https://www.naver.com");
			HttpsURLConnection conn=url.openConnection();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

}
