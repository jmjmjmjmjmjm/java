package ch02;

//heap
class Zelot{
	String name ="질럿";
	int attack =10;
	int armor =5;
}

public class Var08 {
	public static void main(String[] args) {
		Zelot z = new Zelot(); //static이 아닌heap을 부를때
		System.out.println(z.name);
		System.out.println(z.attack);
		System.out.println(z.armor);

	}
}//end
