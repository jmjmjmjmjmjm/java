package composit;

import lombok.Data;

@Data

public class Coke {
		
	private int price;
	private String desc;
	
	public Coke(int price, String desc) {

		this.price = price;
		this.desc = desc;
		System.out.println("코크가 만들어졌습니다.");
	}
	
	public Coke() {
	this(1000,"코카콜라");
		
	}
	
	
	
	
	public static void main(String[] args) {

	}





	

}
