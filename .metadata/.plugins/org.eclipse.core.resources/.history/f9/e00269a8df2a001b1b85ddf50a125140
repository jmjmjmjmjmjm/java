package book;

public class Platics {
	public static void main(String[] args) {
		String a = new String("C#");
		String b = new String(",C++");
		
		System.out.println(a + "의 길이는"  + a.length()); //문자열 길이(개수)
		System.out.println(a.contains("#")); //문자열 포함관계
		
		a=a.concat(b); //문자열 연결
		
		
	}

}
